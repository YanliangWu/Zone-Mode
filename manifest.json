{
  "manifest_version": 2,

  "name": "Zoom-Mode",
  "description": "This Extension is an customizable address filter",
  "version": "1.0",

  "browser_action": {
    "default_icon": "inactive.png"

  },


  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "background": {
    "scripts": ["backGround.js"]
  },
  "chrome_url_overrides": {
    "newtab" : "newtab.html"
  }

}

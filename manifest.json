{
  "manifest_version": 2,

  "name": "Zone-Mode",
  "description": "This Extension is an customizable address filter",
  "version": "0.9",
  "options_page": "setting.html",

  "browser_action": {
    "default_icon": "inactive.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "background": {
    "scripts": ["backGround.js"]
  }

}
